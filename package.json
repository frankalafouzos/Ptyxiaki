    "url": "https://github.com/frankalafouzos/Ptyxiaki/issues"
  },
  "homepage": "https://github.com/frankalafouzos/Ptyxiaki#readme",
  "dependencies": {
    "@fullcalendar/daygrid": "^6.1.15",
    "@fullcalendar/interaction": "^6.1.15",
    "@fullcalendar/react": "^6.1.15",
    "@fullcalendar/timegrid": "^6.1.15",
    "concurrently": "^8.2.2",
    "express": "^4.18.2",
    "multer": "^1.4.5-lts.1",
    "nodemon": "^3.0.3",
    "openai": "^4.76.0",
    "react-scripts": "^5.0.1",
    "react-slick": "^0.30.3",
    "slick-carousel": "^1.8.1"
